def divider():
    out = "+"
    i = 0
    while i<12:
        if not(i%2):
            out+="-"
        else:
            out+="*"
        i+=1
    out+="+"
    print (out)
def headtail():
    i = 0
    sp = 5
    while i<5:
        out = ""
        j = 0
        while j <sp:
            out +=" "
            j+=1
        j = 0
        while j<1+i:
            out+="/"
            j+=1
        out+="**"
        j = 0
        while j<1+i:
            out+="\\"
            j+=1
        sp-=1
        print (out)
        i+=1
def body(n):
