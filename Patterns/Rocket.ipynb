        else:
            dots = 0
            sym_size = 3
            sym = "\\/"
        while j <6:
            out = "|"
            k =0
            while k<2:
                l = 0
                while l<dots:
                    out+="."
                    l+=1
                l = 0
                while l<sym_size:
                    out+=sym
                    l+=1
                l = 0
                while l<dots:
                    out+="."
                    l+=1
                k+=1
            out+="|"
            print (out)
            j+=1
            if (j<3 and not(i%2)) or (j>3 and i%2):
                dots-=1
                sym_size+=1
            elif j==3 and not(i%2):
                sym = "\\/"
            elif j==3 and i%2:
                sym = "/\\"
            elif (j>3 and not(i%2)) or (j<3 and i%2):
                sym_size-=1
                dots+=1
        divider()
        i+=1
def main():
    n = int(input("Enter desired size of rocket: "))
